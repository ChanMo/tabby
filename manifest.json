{
  "name": "__MSG_name__",
  "version": "1.0.1",
  "description": "__MSG_description__",
  "manifest_version": 3,
  "author": "chan.mo@outlook.com",
  "homepage_url": "https://github.com/ChanMo/tabby.git",
  "default_locale": "en",
  "content_scripts": [
    {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["script.js"],
    "css": ["style.css"]
  }
  ],
  "permissions": ["activeTab", "storage"],
  "icons": {
    "16": "logo_16.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}
